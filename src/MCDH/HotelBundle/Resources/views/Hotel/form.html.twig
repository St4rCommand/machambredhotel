<div class="well">
	{{ form_start(form,{}) }}
	{{ form_errors(form) }}
	
	<fieldset id="form-hotel-contact">
		<legend>{{ 'coords'|trans }}</legend>
		<div class="form-group row">
			{{ form_label(form.name, "name"|trans, {'label_attr': {'class': 'col-lg-offset-1 col-sm-2 control-label'}}) }}
			
			<div class="col-sm-4">
				{{ form_widget(form.name, {'attr': {'class': 'form-control'}}) }}
			</div>
			
			{{ form_errors(form.name, {'attr': {'class': 'col-sm-3'}}) }}
		</div>
		
		<div class="form-group row">
			{{ form_label(form.address, "adress"|trans, {'label_attr': {'class': 'col-lg-offset-1 col-sm-2 control-label'}}) }}
			
			<div class="col-sm-4">
				{{ form_widget(form.address, {'attr': {'class': 'form-control'}}) }}
			</div>
			
			{{ form_errors(form.address, {'attr': {'class': 'col-sm-3'}}) }}
		</div>
		
		<div class="form-group row">
			{{ form_label(form.postcode, "post.code"|trans, {'label_attr': {'class': 'col-lg-offset-1 col-sm-2 control-label'}}) }}
			
			<div class="col-sm-4">
				{{ form_widget(form.postcode, {'attr': {'class': 'form-control'}}) }}
			</div>
			
			{{ form_errors(form.postcode, {'attr': {'class': 'col-sm-3'}}) }}
		</div>
		
		<div class="form-group row">
			{{ form_label(form.city, "city"|trans, {'label_attr': {'class': 'col-lg-offset-1 col-sm-2 control-label'}}) }}
			
			<div class="col-sm-4">
				{{ form_widget(form.city, {'attr': {'class': 'form-control'}}) }}
			</div>
			
			{{ form_errors(form.city, {'attr': {'class': 'col-sm-3'}}) }}
		</div>
		
		<div class="form-group row">
			{{ form_label(form.country, "country"|trans, {'label_attr': {'class': 'col-lg-offset-1 col-sm-2 control-label'}}) }}
			
			<div class="col-sm-4">
				{{ form_widget(form.country, {'attr': {'class': 'form-control'}}) }}
			</div>
			
			{{ form_errors(form.country, {'attr': {'class': 'col-sm-3'}}) }}
		</div>
		
		<div class="form-group row">
			{{ form_label(form.email, "email"|trans, {'label_attr': {'class': 'col-lg-offset-1 col-sm-2 control-label'}}) }}
			
			<div class="col-sm-4">
				{{ form_widget(form.email, {'attr': {'class': 'form-control'}}) }}
			</div>
			
			{{ form_errors(form.email, {'attr': {'class': 'col-sm-3'}}) }}
