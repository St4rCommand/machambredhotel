		<div class="form-group row">
			{{ form_label(form.phoneNumber, "phone"|trans, {'label_attr': {'class': 'col-lg-offset-1 col-sm-2 control-label'}}) }}
			
			<div class="col-sm-4">
				{{ form_widget(form.phoneNumber, {'attr': {'class': 'form-control'}}) }}
			</div>
			
			{{ form_errors(form.phoneNumber, {'attr': {'class': 'col-sm-3'}}) }}
		</div>
	</fieldset>
	
	<fieldset>
		<legend>{{ 'details'|trans }}</legend>
		
		<div class="form-group row">
			{{ form_label(form.floor, 'num.floor'|trans, {'label_attr': {'class': 'col-lg-offset-1 col-sm-2 control-label'}}) }}
			
			<div class="col-sm-4">
				{{ form_widget(form.floor, {'attr': {'class': 'form-control'}}) }}
			</div>
			
			{{ form_errors(form.floor, {'attr': {'class': 'col-sm-3'}}) }}
		</div>
		
		<div class="form-group row">
			{{ form_label(form.image, "picture"|trans, {'label_attr': {'class': 'col-lg-offset-1 col-sm-2 control-label'}}) }}
			
			<div class="col-sm-4">
				{{ form_widget(form.image, {'attr': {'class': 'form-control'}}) }}
			</div>
			
			{{ form_errors(form.image, {'attr': {'class': 'col-sm-3'}}) }}
		</div>
		
		<div class="form-group row">
			{{ form_label(form.description, "description"|trans, {'label_attr': {'class': 'col-lg-offset-1 col-sm-2 control-label'}}) }}
			
			<div class="col-sm-4">
				{{ form_widget(form.description, {'attr': {'class': 'form-control'}}) }}
			</div>
			
			{{ form_errors(form.description, {'attr': {'class': 'col-sm-3'}}) }}
