{% extends "MCDHHotelBundle:Hotel:layout.html.twig" %}

{% block title %}
	{{ 'chambre'|trans }} - {{ parent() }}
{% endblock %}

{% block mcdh_hotel_breadcrumb_trail %}
	{{ parent() }} > {{ 'room.ajout'|trans }}
{% endblock %}


{% if is_granted('ROLE_HOTELKEEPER') %}
	{% block mcdh_hotel_content %}
		{{ parent() }}
		{{ include("MCDHHotelBundle:Room:form.html.twig") }}
	{% endblock %}
{% endif %}